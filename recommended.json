{
    "compilerOptions": {
        "declaration": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitOverride": true
    }
}
